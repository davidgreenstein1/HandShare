{
    "env": {
        "browser": true,
        "node": true,
        "jquery": true,

    },
    "globals": {
        "window": true
    },
    "rules": {
        "use-strict": 0,
        "strict": 0,
        "global-strict": 0,
        "no-extra-strict": 0,
        "no-undef": 1,
        "no-unused-vars": 1,
        "no-multi-spaces": 0,
        "no-trailing-spaces": 1
    }
}